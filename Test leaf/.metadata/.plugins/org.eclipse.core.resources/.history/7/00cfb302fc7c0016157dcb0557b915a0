package my_java;

import java.io.IOException;

public class LaunchingExternalApps {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Runtime runtime=Runtime.getRuntime()
				
				try{
					
					runtime.exec("notepad.exe");
				}
		
		catch (IOException e)
        {
            e.printStackTrace();
        }
	}

}
